{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "affix_web_5000",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port",
                "5000"
            ]
        },
        {
            "name": "affix_web (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--web-port",
                "5000"
            ]
        },
        {
            "type": "node-terminal",
            "name": "JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        }
    ]
}